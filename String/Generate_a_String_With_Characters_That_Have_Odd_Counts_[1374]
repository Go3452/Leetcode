class Solution {
    public String generateTheString(int n) {
        String ans="";
        if(n%2==1)
        {
         for(int i=0;i<n;i++)
          ans+="g";
        }
        else
        {
         for(int i=0;i<n-1;i++)
          ans+="g";
         ans+="m";
        }
        return ans;
    }
}
