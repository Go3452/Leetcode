class Solution {
    public String finalString(String s) {
        String ans="";
        for(int i=0;i<s.length();i++)
        {
         if(s.charAt(i)=='i')
         {
          String a="";
          for(int j=0;j<ans.length();j++)
          {
           a=ans.charAt(j)+a;
          }
          ans=a;
         }
         else
          ans+=s.charAt(i);
        }
        return ans;
    }
}
